<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="installment_sequence" model="ir.sequence">
            <field name="name">Installment Sequence Number</field>
            <field name="code">installment.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">INV/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="quarterly_sequence" model="ir.sequence">
            <field name="name">Quarterly Sequence Number</field>
            <field name="code">quarterly.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">INV/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>